<!-- BuildingBlockEditPanel.ui.xml -->
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

<ui:style>
.mainPanel {
  padding: 5px;
}
.panel {
  background-color: #f8f8f8;
  margin: 2px 2px 2px 10px;
  padding: 10px 5px 5px 5px;
}
.buttonPanel{
  float: right;
}
.noItems {
  display: none;
  border: 1px solid #888;
  padding: 10px 10px 10px 10px;
  margin: 25px 5px 5px 5px;
}
.title {
  padding: 2px 2px 20px 2px;
}
</ui:style>

<g:HTMLPanel styleName="{style.mainPanel}">
  <div class="{style.panel}">
    <u>Options:</u>
    <dl>
    	<dt><g:CheckBox ui:field="materializeCheckBox" formValue="materialize">Materialize</g:CheckBox></dt>
    	<dd>Materializes the result when publishing the strategy. This is a manual optimization option 
    	 that should normally not be used. It might cause incorrect results.</dd>
    </dl>
  </div>
</g:HTMLPanel>

</ui:UiBinder>