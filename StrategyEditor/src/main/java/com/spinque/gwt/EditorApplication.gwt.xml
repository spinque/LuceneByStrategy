<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='editorApp'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.core.Core"/>
  <inherits name="com.google.gwt.json.JSON"/>
  <inherits name="com.google.gwt.user.History"/>
  <inherits name="com.google.gwt.xml.XML" />

  <inherits name='com.google.gwt.uibinder.UiBinder' />
  <set-configuration-property name="UiBinder.useSafeHtmlTemplates" value="true" /> 
  <set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
  
  <inherits name='org.vectomatic.libgwtsvg' />

  <inherits name="com.spinque.gwt.utils.Utils" />  
  <inherits name="com.spinque.rest.REST" />
  <inherits name="com.spinque.gwt.args.Args" />
  
  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.chrome.Chrome'/>

  <!-- Other module inherits                                      -->
  
  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.spinque.gwt.client.EditorApplication' />
  
  <extend-property name="locale" values="en"/>
    
  <!-- allows for reading resources on the client -->
  <inherits name="com.google.gwt.resources.Resources" />
   
   <!-- Inherit gwt-dnd support                   -->
   <inherits name='com.allen_sauer.gwt.dnd.gwt-dnd'/>
   
</module>
